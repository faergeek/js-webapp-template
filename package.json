    "@babel/runtime": "^7.7.4",
    "@curi/router": "^2.0.1",
    "@curi/static": "^2.0.1",
    "@hickory/browser": "^2.0.1",
    "@hickory/in-memory": "^2.0.0",
    "babel-loader": "^8.0.6",
    "core-js": "^3.2.1",
